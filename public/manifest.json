{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "obsidianCaptureTool",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "downloads",
        "tabs"
    ],

    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}